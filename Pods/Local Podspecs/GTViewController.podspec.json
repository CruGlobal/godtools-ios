{
  "name": "GTViewController",
  "version": "4.0.2",
  "summary": "A View Controller that renders God Tools xml packages",
  "description": "                   GTViewController takes a God Tools xml package and renders it for iOS devices.\n\n                   God Tools packages can be downloaded from the God Tools API and can be rendered\n                   on Android too. All God Tools code is open source.\n\n                   If you would like to create your own xml packages the documentation is at godtoolsapp.com/docs\n",
  "homepage": "http://github.com/CruGlobal/GTViewController",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Harro": "michael.harrison@cru.org"
  },
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "git": "https://github.com/CruGlobal/GTViewController.git",
    "tag": "4.0.2"
  },
  "source_files": [
    "GTViewController/Classes/**/*.{h,m}",
    "GTViewController/Headers/*.h"
  ],
  "public_header_files": [
    "GTViewController/Classes/Nav Classes/GTViewController.h",
    "GTViewController/Classes/Custom Classes/GTFileLoader.h",
    "GTViewController/Classes/Nav Classes/GTPageMenuViewController.h",
    "GTViewController/Classes/Custom Classes/GTShareViewController.h",
    "GTViewController/Classes/Parsing Classes/GTAboutViewController.h",
    "GTViewController/Classes/TBXML/TBXML.h"
  ],
  "resources": [
    "GTViewController/Resources/*",
    "GTViewController/Classes/**/*.xib"
  ],
  "frameworks": [
    "QuartzCore",
    "AVFoundation"
  ],
  "libraries": "z",
  "dependencies": {
    "SSCWhatsAppActivity": [
      "~> 1.0"
    ]
  },
  "requires_arc": true
}
